smol = true
Project cate
if(windows) {
    .files = {"src/*.c" "src/windows/*.c"}
    .out = "out/cate.exe"
} else {
    .files = {"src/*.c" "src/posix/*.c"}
    .out = "out/cate"
}
.flags = "-Ofast -march=native -funsafe-math-optimizations -ffast-math -fno-signed-zeros -ffinite-math-only"
.std = "c99"
.incs = {"." "include"}
.defs = {"_DEFAULT_SOURCE"}
.build()
//.install()