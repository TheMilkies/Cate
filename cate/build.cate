Project cate
.compiler = "g++"
.files = recursive("src/**.cpp")
.flags = "-fpermissive -funsafe-math-optimizations -ffast-math -fno-signed-zeros -ffinite-math-only -std=c++17 -Wall -Ofast"
.libs = {"externals/linux_amd64_libfl.a", "stdc++fs"}
.build_directory = "cate/build"
.threading = true
.out = "out/cate"
.build()